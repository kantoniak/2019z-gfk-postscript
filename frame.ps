%!

(data-ps/colors.ps) run
(data-ps/static.ps) run
(data-ps/fish.ps) run

/kwadrat {
    % Kwadrat
    colorSand
    /dx frameWidth totalProgress mul def
    /dy frameHeight totalProgress mul def
    newpath
    dx dy moveto
    0 100 rlineto
    100 0 rlineto
    0 -100 rlineto
    closepath
    stroke
} def

/renderFrame {
    renderAquarium

    % Fish in the middle
    gsave
    frameWidth 2 div frameHeight 2 div translate
    renderFishObject
    grestore

    % Big stone
    gsave
    colorStone
    3420 60 translate
    renderStone
    grestore

} def

renderFrame
showpage
